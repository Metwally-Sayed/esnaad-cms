    {
      "id": "blk-hero-about",
      "name": "Hero: About Esnaad",
      "type": "HERO",
      "variant": "hero-title-only",
      "content": {
        "title": "ABOUT ESNAAD",
        "backgroundColor": "#f8f6f4",
        "textColor": "#111111"
